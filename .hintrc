{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "color: color-mix(in srgb, var(--primary-grey), black 20%)",
          "backdrop-filter",
          "background-color: color-mix(in srgb, var(--primary-beige) 40%, var(--primary-light))",
          "background-color: color-mix(in srgb, var(--label-color), var(--primary-grey))",
          "mask-composite",
          "mask-composite: intersect",
          "user-select",
          "mask-image",
          "scrollbar-color",
          "scrollbar-width",
          "background-color: color-mix(in srgb, transparent 20%, var(--label-color))"
        ]
      }
    ],
    "css-prefix-order": "off",
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ]
  }
}